	nsim_isa_xmac_d16=1
	nsim_isa_mul32x16=1
	nsim_isa_number_of_interrupts=8
	nsim_isa_intvbase_preset=0x0
	nsim_bpu_bc_entries=512
	nsim_bpu_bc_full_tag=0
