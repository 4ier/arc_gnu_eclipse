	nsim_isa_lpc_size=32
	nsim_isa_addr_size=24
	nsim_isa_bitscan_option=0
	nsim_isa_sat=1
	nsim_isa_mult32=1
	nsim_isa_shift_option=2
	nsim_isa_number_of_interrupts=32
	nsim_isa_intvbase_preset=0x0
	dccm_size=0x800
	dccm_base=0x0080_0000
	iccm_size=0x80000
	iccm_base=0
